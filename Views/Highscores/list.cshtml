

@model IEnumerable<mysql_scaffold_dbcontext_test.Models.Highscores>

@{
    ViewData["Title"] = "List";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>List</h1>

<form asp-action="Index" method="get">
    <div class="form-actions no-color">
        <p>
            Find by name: <input type="text" name="SearchString" value="@ViewData["CurrentFilter"]" />
            <input type="submit" value="Search" class="btn btn-default" /> |
            <a asp-action="Index">Back to Full List</a>
        </p>
    </div>
</form>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Userid)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Modeid)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Character)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Level)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Version)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Date)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Time)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TotalPoints)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LongestShot)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TotalDistance)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MaxShotMade)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MaxShotAtt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ConsecutiveShots)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TwoMade)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TwoAtt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ThreeMade)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ThreeAtt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FourMade)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.FourAtt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SevenMade)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SevenAtt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BonusPoints)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MoneyBallMade)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.MoneyBallAtt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Platform)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Userid)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Modeid)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Character)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Level)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Version)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Date)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Time)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TotalPoints)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.LongestShot)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.TotalDistance)
                </td>
                <td>
