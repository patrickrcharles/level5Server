@model IEnumerable<mysql_scaffold_dbcontext_test.Models.nftairdrop.NFTRequest>

@{
    ViewData["Title"] = "Level5 NFT airdrop request";
    Layout = "~/Views/Shared/_Layout_nft.cshtml";
}

<h3>NFT Requests</h3>
<br />
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TokenId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.RequestAddress)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Transferred)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TimeStamp)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TokenId)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.RequestAddress)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Transferred)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TimeStamp)
            </td>
        </tr>
        }
    </tbody>
</table>